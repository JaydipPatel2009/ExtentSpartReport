<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
	
		<!-- report theme -->
		<!-- standard, dark -->
		<theme>standard</theme>
		
		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>
		
		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>
		
		<!-- title of the document -->
		<documentTitle>SalesForce Test Execution report</documentTitle>
		
		<!-- report name - displayed at top-nav -->
		<reportName>SalesForce Test Execution Results</reportName>
		
		<!-- timestamp format -->
		<timeStampFormat>MMM dd, yyyy HH:mm:ss</timeStampFormat>
		
		<!-- settings to enable/disable views -->
		<enableCategoryView>true</enableCategoryView>
		<enableAuthorView>true</enableAuthorView>
		<enableExceptionView>true</enableExceptionView>
		<enableTestRunnerLogsView>true</enableTestRunnerLogsView>
		
		<!-- custom javascript -->
		<scripts>
			<![CDATA[
				$(document).ready(function() {
				    
				});
			]]>
		</scripts>
		
		<!-- custom styles -->
		<styles>
			<![CDATA[
			
			]]>
		</styles>
		
	</configuration>
</extentreports>